<permissions/>
<installUrl>https://github.com/TVLuke/SensorDroneDynamixPlugin/raw/master/dist/org.ambientdynamix.contextplugins.sensordrone_1.0.0.jar</installUrl>
<updateUrl/>
<updateMessage priority="OPTIONAL"/>
</contextPlugin>
